@import "./main.css";

#cpu, #tray, #pulseaudio {
  margin-right: 0px;
  padding-right: 3px;
  border-top-right-radius: 0px;
  border-bottom-right-radius: 0px;
}

#memory, #backlight {
  margin-left: 0;
  margin-right: 0;
  border-radius: 0;
  padding-left: 3px;
  padding-right: 3px;
}

#battery, #custom-swaync, #language {
  margin-left: 0px;
  padding-left: 3px;
  border-top-left-radius: 0px;
  border-bottom-left-radius: 0px;
}
